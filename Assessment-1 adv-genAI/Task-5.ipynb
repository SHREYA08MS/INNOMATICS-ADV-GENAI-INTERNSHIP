   "source": [
    "Detect error messages from system logs.\n",
    "\n",
    "logs = [\"INFO\", \"ERROR\", \"WARNING\", \"ERROR\"]\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "510797bc",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Total ERROR entries: 2\n"
     ]
    }
   ],
   "source": [
    "logs = [\"INFO\", \"ERROR\", \"WARNING\", \"ERROR\"]\n",
    "\n",
    "error_count = 0\n",
    "\n",
    "for log in logs:\n",
    "    if log == \"ERROR\":\n",
    "        error_count += 1\n",
    "\n",
    "print(\"Total ERROR entries:\", error_count)"
   ]
